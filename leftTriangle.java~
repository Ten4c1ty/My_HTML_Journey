import java.util.Scanner;
public class leftTriangle{
    public static void main(String [] args){
        Scanner sc = new Scanner(System.in);
        int height = sc.nextInt();
        int m = height-1;
        while(m>=0){
        for(int i = 1;i<=height-m;i++){
            System.out.print(i);
        }
            /*for(int l = 1;l<m;l++){
                System.out.print(" ");
            }*/
            System.out.println("");
            m--;
        }
    }
    }