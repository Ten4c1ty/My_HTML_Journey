//import java.util.Scanner;
public class Palindrome{
    public static void main(String [] args){
        //Scanner sc = new Scanner(System.in);
        for(int i = 1; i<=7;i++){
            System.out.print(i);
        }
        for(int k = 6; k<7;k--){
            System.out.print(k);
        }
    }
}