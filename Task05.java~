import java.util.Scanner;
public class Task05{
    public static void main(String [] args){
        Scanner sc = new Scanner(System.in);
        int i = 1;
        while(i<=10){
            System.out.println("Enter Number: ");
            int number = sc.nextInt();
            System.out.println("Sum = "+number);
            number += number;
        }
    }
}