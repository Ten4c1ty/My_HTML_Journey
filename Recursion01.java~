public class Recursion01{
    public static void oneToN(int n){
        if (n < 1) {
            return;
        }
        oneToN(n - 1);
        System.out.print(n + " ");
    }
    public static void main(String [] args){
        System.out.println("Sample Input: N = 5");
        System.out.print("Output: ");
        oneToN(5);
        System.out.println();
        System.out.println("\nSample Input: N = 11");
        System.out.print("Output: ");
        oneToN(11);
        System.out.println();
    }
}