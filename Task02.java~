public class Task02{
    public static void main(String [] args){
        for(int i=1; i<=600; i++){
            if(i%7 == 0 || i%9 == 0){
                if(i%7 == 0 && i%9 == 0){
                    int both = i++;
                }
            int onlyonce = (i++)-both;
            }
        }
    }
}