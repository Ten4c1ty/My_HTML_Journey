        System.out.println("Please enter your password: ");
        String pass = sc.nextLine();
        String new_pass = "";
        if(pass.length() >= 8){
            for(int i = 0; i < pass.length(); i++){
                int convert = (int) pass.charAt(i);
                if((convert >= 65 && convert <= 90) && (convert >= 97 && convert <= 122) && (convert >= 48 && convert <= 57)){
                    
                }
            }
        }
    }
}