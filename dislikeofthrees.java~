public class dislikeofthrees{
    public static void main(String [] args){
        int i = 1;
        int j = 1000;
        for(int k = i; k <= j; k++){
            if(k%3 == 0 || k%10 == 3){
                k--;
            }
            else{
                System.out.println(k);
            }
        }
    }
}