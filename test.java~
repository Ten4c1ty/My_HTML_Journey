import java.util.Scanner;

public class test {
    public static void main(String[] args) {
        Scanner scan = new Scanner(System.in);
        int num = scan.nextInt();
        int temp1 = 0;
        
        while(num != 0) {
            temp1 = num % 10;
            num /= 10;
            System.out.print(temp1 + " ");
        }
    }
}
